{
  "name": "three-seed",
  "version": "0.3.2",
  "description": "Three.js starter project boilerplate bundled with webpack",
  "keywords": [
    "webgl",
    "boilerplate",
    "three.js",
    "es6",
    "webpack"
  ],
  "scripts": {
    "start": "webpack-dev-server --mode development",
    "prebuild": "rimraf ./build && mkdir ./build",
    "build": "webpack --mode production",
    "format": "prettier --write '*.js' 'src/**/*.js'"
  },
  "dependencies": {
    "dat.gui": "^0.7.7",
    "three": "^0.115.0",
    "three-obj-loader": "^1.1.3",
    "webpack-cli": "^3.3.11"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-loader": "^8.1.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.2.0",
    "raw-loader": "^4.0.1",
    "rimraf": "^3.0.2",
    "webpack": "^4.43.0",
    "prettier": "^2.0.4",
    "webpack-dev-server": "^3.10.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
